{
  "name": "electron-app-template",
  "version": "0.0.1",
  "description": "Electron App Template",
  "main": "./app/main.js",
  "scripts": {
    "refresh": "electron .",
    "watch": "start lapis && npm-watch",
    "watch-alt": "npm-watch | lapis",
    "dist": "electron-packager . --out=dist --overwrite --version-string.FileDescription=electron-app-template"
  },
  "repository": "https://github.com/entozoon/electron-app-template",
  "keywords": [
    "lists",
    "imdb",
    "movies"
  ],
  "author": "MichaelCook <entozoon@gmail.com>",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.4.1",
    "electron-packager": "^8.6.0"
  },
  "dependencies": {
    "electron-packager": "^8.6.0",
    "lapis-compiler": "^1.6.4",
    "npm-watch": "^0.1.8"
  },
  "watch": {
    "refresh": {
      "patterns": "*",
      "extensions": "js,jsx,css",
      "quiet": true
    }
  }
}
